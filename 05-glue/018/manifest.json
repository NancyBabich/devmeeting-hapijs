{
  "connections": [
    {
      "port": 3000
    }
  ],
  "registrations": [
    {
      "plugin": "inert"
    },
    {
      "plugin": "vision"
    },
    {
      "plugin": {
        "register": "good",
        "options": {
          "ops": {
            "interval": 5000
          },
          "reporters": {
            "consoleReporter": [{
              "module": "good-squeeze",
              "name": "Squeeze",
              "args": [{ "log": "*", "response": "*" }]
            }, {
              "module": "good-console"
            }, "stdout"]
          }
        }
      }
    },
    {
      "plugin": "./routes/index"
    },
    {
      "plugin": "./routes/api/products",
      "options": {
        "routes": {
          "prefix": "/api/products"
        }
      }
    },
    {
      "plugin": "./routes/static"
    }
  ]
}
